{
  "name": "keyboardninja",
  "version": "0.2.0",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/testing": "^7.4.2",
    "@sendgrid/mail": "^6.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "email-validator": "^2.0.4",
    "express": "4.16.4",
    "googleapis": "^27.0.0",
    "mongoose": "^5.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "razzle": "3.0.0",
    "razzle-heroku": "^3.0.0",
    "razzle-plugin-typescript": "^3.1.6",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-hook-form": "^6.4.1",
    "react-router-dom": "5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^6.0.13",
    "@storybook/addon-knobs": "^6.0.13",
    "@storybook/addon-links": "^6.0.13",
    "@storybook/addon-viewport": "^6.0.13",
    "@storybook/addons": "^6.0.13",
    "@storybook/react": "^6.0.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/email-validator": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/mongoose": "^5.7.36",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/supertest": "^2.0.10",
    "babel-loader": "^8.0.5",
    "express-list-routes": "^0.1.4",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "3.9",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "ts-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/*"
    ],
    "moduleNameMapper": {
      "@server(.*)": "<rootDir>/src/server$1",
      "@client(.*)": "<rootDir>/src/client$1",
      "@src(.*)": "<rootDir>/src$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "verbose": true
  },
  "scripts": {
    "start": "razzle start",
    "dev": "razzle start -- --inspect-brk",
    "build": "yarn version && razzle build",
    "test": "razzle test",
    "deploy": "git push heroku master",
    "start:prod": "NODE_ENV=production node build/server.js",
    "sb": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  }
}
